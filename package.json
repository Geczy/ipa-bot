    "mongo": "^0.1.0",
    "sharp": "0.33.0-alpha.9",
    "telegram": "^2.19.8"
  },
  "trustedDependencies": [
    "frida"
  ],
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=19.0.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.8.8",
    "bun-types": "^1.0.7",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
